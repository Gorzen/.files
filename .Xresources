!! Colorscheme

! special
*.foreground:   #dcdccc
*.background:   #06060A
*.cursorColor:  #f0f1f1

! black
*.color0:       #000000
*.color8:       #828282

! red
*.color1:       #cc0403
*.color9:       #f2201f

! green
*.color2:       #19cb00
*.color10:      #23fd00

! yellow
*.color3:       #cecb00
*.color11:      #fffd00

! blue
*.color4:       #0d73cc
*.color12:      #1a8fff

! magenta
*.color5:       #cb1ed1
*.color13:      #fd28ff

! cyan
*.color6:       #0dcdcd
*.color14:      #14ffff

! white
*.color7:       #dddddd
*.color15:      #ffffff

xterm*metaSendsEscape: true

!! URxvt Appearance
URxvt.background:        #06060A
URxvt.font:              xft:monospace:size=16
urxvt*transparent:       true
urxvt*shading:           20

URxvt.letterSpace:       0
URxvt.lineSpace:         0
URxvt.internalBorder:    5
URxvt.cursorBlink:       true
URxvt.cursorUnderline:   false
URxvt.scrollBar:         false
URxvt.saveline:          50000
URxvt.iso14755:          false
URxvt.iso14755_52:       false

!! Scrollback
URxvt*scrollTtyOutput:   false
URxvt*scrollTtyKeypress: true
URxvt*scrollWithBuffer:  true

!! Extensions
URxvt.perl-ext-common:   default,clipboard,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:           #4682B4
!! clipboard
URxvt.keysym.M-c:        perl:clipboard:copy
URxvt.keysym.M-v:        perl:clipboard:paste
URxvt.clipboard.autocopy: true
!!url-select:
URxvt.url-select.underline: true
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.keysym.M-u:        perl:url-select:select_next
!! keyboard-select
URxvt.keysym.M-k:        perl:keyboard-select:activate
URxvt.keysym.M-s:        perl:keyboard-select:search
URxvt.keyboard-select.clipboard: true
!! resize-font
URxvt.resize-font.bigger:  M-1
URxvt.resize-font.smaller: M-2
!! matcher
URxvt.urlLauncher:      /usr/bin/xdg-open
URxvt.underlineURLs:     true
URxvt.matcher.button:    1
URxvt.urlButton:         1

!! URxvt.keysym.Control-Right: \033[1;5C
!! URxvt.keysym.Control-Left: \033[1;5D
